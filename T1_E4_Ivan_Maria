n = 0 : 1 : 20; %se genereaza vectorul n cu val de la 0 la 20

m = -5 : 1 : 15; %se genereaza vectorul m cu val de la -5 la 15

z = zeros(21,1);%se genereaza vect z ce initial are toate componentele 0

z(6) = 1;%pe poz 6 se pune val 1
%se reprezinta grafic dependenta lui z de n si m
subplot(2,1,1);

stem(n,z);

subplot(2,1,2);

stem(m,z);
